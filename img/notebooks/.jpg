<script>


	function getQueryParams(qs) {

		if (qs.length == 0)
			return false;

		qs = qs.split("+").join(" ");

		var params = {}, tokens,
			re = /[?&]?([^=]+)=([^&]*)/g;

		while (tokens = re.exec(qs)) {
			params[decodeURIComponent(tokens[1])]
				= decodeURIComponent(tokens[2]);
		}

		return params;
	}

	var sectionAjaxContainer = $('.catalog-top.flat');

	$(document).ready(function() {



		if($.cookie('holidaydelivery2017') != 'checked'){
        	$('.delivery_holiday').show();
		}

        
        
        /*ga('send', 'event', 'Meizu M3s Action', 'window opened', '/');
        dataLayer.push({
            'event': 'Meizu M3sA',
            'eventCategory': 'Meizu M3s Action',
            'eventAction': 'window opened'
        });*/
    


		
		$('.delivery_holiday_close').on('click', function() {
			
			$('.delivery_holiday').hide();
			$.cookie('holidaydelivery2017', 'checked', {expires: 100, path: '/'});
		});


		//prevent click on disabled pagination button
		$('.section-navigation a.disabled').on('click', function(event) {
			event.preventDefault();
			event.stopPropagation();
		});

		// ajax pagination
		$('#ajaxload').on('click', function() {

			//ga
			ga('send', 'event', 'Плитка товаров/дозагрузка товаров', 'click-Ajax-button', 'Показать еще 30 товаров');

			$(this).addClass('load');

			//default params
			var self = this,
				curUrl = location.pathname,
				navNum = $(this).data('navnum'),
				navPage = $(this).data('navpage'),
				navQuery = $(this).data('query'),
				navPageCount = $(this).data('navpagecount'),
				postUrl = curUrl + "?" + navQuery + "PAGEN_" + navNum + "=" + ++navPage;

			//get current pagen from url
			var urlParams = getQueryParams(location.search),
				pagen = urlParams['PAGEN_' + navNum];

			$.get(
				postUrl,
				{
					'CUR_PAGEN': (pagen || 1)
				},
				function(data) {
					// append loadede data
					$(data).find('.catalog-top.flat .module_item, .catalog-top.flat .plitka_hit_item').appendTo( sectionAjaxContainer );


					// update pagination
					$(self).parent().find('.section-navigation').html( $(data).find('.section-nav-string .section-navigation').html() )

					// update cur page
					$(self).data('navpage', navPage);
					
					$('.bat_icons').battips();
					
					//remove .load
					$(self).removeClass('load');
					if ($(self).data('navpagecount') == navPage)
						$(self).hide();
				}
			);
		});
		sendCitrusImpressions();
	});
